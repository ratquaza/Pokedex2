<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Ratquaza.Pokedex2</RootNamespace>
    <Authors>Anky Yawanis</Authors>
    <Company>ratquaza</Company>
    <Description>Lightweight open source C# Library Pokedex that loads Pokemon data through PokeAPI.</Description>
    <PackageProjectUrl>https://github.com/ratquaza/Pokedex2</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ratquaza/Pokedex2</RepositoryUrl>
    <PackageId>Ratquaza.Pokedex2</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
